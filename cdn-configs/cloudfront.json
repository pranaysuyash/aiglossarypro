{
  "name": "AIGlossaryPro CloudFront CDN Configuration",
  "version": "1.0.0",
  "description": "AWS CloudFront CDN configuration for optimal performance and security",
  "distribution": {
    "aliases": [
      "aiglossarypro.com",
      "www.aiglossarypro.com"
    ],
    "comment": "AIGlossaryPro Production CDN",
    "default_cache_behavior": {
      "target_origin_id": "aiglossarypro-origin",
      "viewer_protocol_policy": "redirect-to-https",
      "cache_policy_id": "custom-assets-cache-policy",
      "origin_request_policy_id": "custom-origin-request-policy",
      "response_headers_policy_id": "custom-security-headers-policy",
      "compress": true,
      "smooth_streaming": false,
      "field_level_encryption_id": "",
      "realtime_log_config_arn": "",
      "trusted_signers": {
        "enabled": false,
        "quantity": 0
      },
      "trusted_key_groups": {
        "enabled": false,
        "quantity": 0
      },
      "allowed_methods": {
        "quantity": 7,
        "items": [
          "GET",
          "HEAD",
          "OPTIONS",
          "PUT",
          "POST",
          "PATCH",
          "DELETE"
        ],
        "cached_methods": {
          "quantity": 2,
          "items": [
            "GET",
            "HEAD"
          ]
        }
      },
      "forwarded_values": {
        "query_string": false,
        "cookies": {
          "forward": "none"
        },
        "headers": {
          "quantity": 0
        }
      },
      "min_ttl": 0,
      "default_ttl": 86400,
      "max_ttl": 31536000
    },
    "cache_behaviors": [
      {
        "path_pattern": "assets/js/*.js",
        "target_origin_id": "aiglossarypro-origin",
        "viewer_protocol_policy": "redirect-to-https",
        "cache_policy_id": "static-assets-cache-policy",
        "compress": true,
        "min_ttl": 31536000,
        "default_ttl": 31536000,
        "max_ttl": 31536000,
        "allowed_methods": {
          "quantity": 2,
          "items": [
            "GET",
            "HEAD"
          ]
        }
      },
      {
        "path_pattern": "assets/css/*.css",
        "target_origin_id": "aiglossarypro-origin",
        "viewer_protocol_policy": "redirect-to-https",
        "cache_policy_id": "static-assets-cache-policy",
        "compress": true,
        "min_ttl": 31536000,
        "default_ttl": 31536000,
        "max_ttl": 31536000,
        "allowed_methods": {
          "quantity": 2,
          "items": [
            "GET",
            "HEAD"
          ]
        }
      },
      {
        "path_pattern": "assets/images/*",
        "target_origin_id": "aiglossarypro-origin",
        "viewer_protocol_policy": "redirect-to-https",
        "cache_policy_id": "static-assets-cache-policy",
        "compress": true,
        "min_ttl": 31536000,
        "default_ttl": 31536000,
        "max_ttl": 31536000,
        "allowed_methods": {
          "quantity": 2,
          "items": [
            "GET",
            "HEAD"
          ]
        }
      },
      {
        "path_pattern": "api/*",
        "target_origin_id": "aiglossarypro-origin",
        "viewer_protocol_policy": "redirect-to-https",
        "cache_policy_id": "no-cache-policy",
        "origin_request_policy_id": "pass-all-headers-policy",
        "compress": true,
        "min_ttl": 0,
        "default_ttl": 0,
        "max_ttl": 0,
        "allowed_methods": {
          "quantity": 7,
          "items": [
            "GET",
            "HEAD",
            "OPTIONS",
            "PUT",
            "POST",
            "PATCH",
            "DELETE"
          ]
        }
      },
      {
        "path_pattern": "*.html",
        "target_origin_id": "aiglossarypro-origin",
        "viewer_protocol_policy": "redirect-to-https",
        "cache_policy_id": "html-cache-policy",
        "compress": true,
        "min_ttl": 0,
        "default_ttl": 3600,
        "max_ttl": 86400,
        "allowed_methods": {
          "quantity": 3,
          "items": [
            "GET",
            "HEAD",
            "OPTIONS"
          ]
        }
      }
    ],
    "origins": [
      {
        "id": "aiglossarypro-origin",
        "domain_name": "your-origin-domain.com",
        "origin_path": "",
        "custom_origin_config": {
          "http_port": 80,
          "https_port": 443,
          "origin_protocol_policy": "https-only",
          "origin_ssl_protocols": {
            "quantity": 3,
            "items": [
              "TLSv1.2",
              "TLSv1.3"
            ]
          },
          "origin_read_timeout": 30,
          "origin_keepalive_timeout": 5
        }
      }
    ],
    "origin_groups": [],
    "enabled": true,
    "is_ipv6_enabled": true,
    "http_version": "http2and3",
    "price_class": "PriceClass_All",
    "viewer_certificate": {
      "acm_certificate_arn": "arn:aws:acm:us-east-1:123456789012:certificate/12345678-1234-1234-1234-123456789012",
      "ssl_support_method": "sni-only",
      "minimum_protocol_version": "TLSv1.2_2021",
      "certificate_source": "acm"
    },
    "restrictions": {
      "geo_restriction": {
        "restriction_type": "none"
      }
    },
    "web_acl_id": "",
    "custom_error_responses": [
      {
        "error_code": 404,
        "response_page_path": "/404.html",
        "response_code": "404",
        "error_caching_min_ttl": 3600
      },
      {
        "error_code": 403,
        "response_page_path": "/403.html",
        "response_code": "403",
        "error_caching_min_ttl": 3600
      },
      {
        "error_code": 500,
        "response_page_path": "/500.html",
        "response_code": "500",
        "error_caching_min_ttl": 300
      }
    ],
    "logging": {
      "enabled": true,
      "include_cookies": false,
      "bucket": "aiglossarypro-cloudfront-logs.s3.amazonaws.com",
      "prefix": "cloudfront-logs/"
    },
    "tags": {
      "Environment": "production",
      "Project": "AIGlossaryPro",
      "Owner": "pranay",
      "CostCenter": "cdn"
    }
  },
  "cache_policies": {
    "static-assets-cache-policy": {
      "name": "AIGlossaryPro-StaticAssets",
      "comment": "Cache policy for static assets (JS, CSS, images)",
      "default_ttl": 31536000,
      "max_ttl": 31536000,
      "min_ttl": 31536000,
      "parameters_in_cache_key_and_forwarded_to_origin": {
        "enable_accept_encoding_gzip": true,
        "enable_accept_encoding_brotli": true,
        "query_strings_config": {
          "query_string_behavior": "none"
        },
        "headers_config": {
          "header_behavior": "none"
        },
        "cookies_config": {
          "cookie_behavior": "none"
        }
      }
    },
    "html-cache-policy": {
      "name": "AIGlossaryPro-HTML",
      "comment": "Cache policy for HTML files",
      "default_ttl": 3600,
      "max_ttl": 86400,
      "min_ttl": 0,
      "parameters_in_cache_key_and_forwarded_to_origin": {
        "enable_accept_encoding_gzip": true,
        "enable_accept_encoding_brotli": true,
        "query_strings_config": {
          "query_string_behavior": "all"
        },
        "headers_config": {
          "header_behavior": "whitelist",
          "headers": [
            "Accept-Language",
            "Authorization"
          ]
        },
        "cookies_config": {
          "cookie_behavior": "all"
        }
      }
    },
    "no-cache-policy": {
      "name": "AIGlossaryPro-NoCache",
      "comment": "No cache policy for API endpoints",
      "default_ttl": 0,
      "max_ttl": 0,
      "min_ttl": 0,
      "parameters_in_cache_key_and_forwarded_to_origin": {
        "enable_accept_encoding_gzip": true,
        "enable_accept_encoding_brotli": true,
        "query_strings_config": {
          "query_string_behavior": "all"
        },
        "headers_config": {
          "header_behavior": "whitelist",
          "headers": [
            "*"
          ]
        },
        "cookies_config": {
          "cookie_behavior": "all"
        }
      }
    }
  },
  "origin_request_policies": {
    "custom-origin-request-policy": {
      "name": "AIGlossaryPro-OriginRequest",
      "comment": "Custom origin request policy",
      "headers_config": {
        "header_behavior": "whitelist",
        "headers": [
          "Accept-Language",
          "User-Agent",
          "Referer"
        ]
      },
      "cookies_config": {
        "cookie_behavior": "all"
      },
      "query_strings_config": {
        "query_string_behavior": "all"
      }
    },
    "pass-all-headers-policy": {
      "name": "AIGlossaryPro-PassAllHeaders",
      "comment": "Pass all headers to origin (for API endpoints)",
      "headers_config": {
        "header_behavior": "whitelist",
        "headers": [
          "*"
        ]
      },
      "cookies_config": {
        "cookie_behavior": "all"
      },
      "query_strings_config": {
        "query_string_behavior": "all"
      }
    }
  },
  "response_headers_policies": {
    "custom-security-headers-policy": {
      "name": "AIGlossaryPro-SecurityHeaders",
      "comment": "Security headers policy",
      "custom_headers": [
        {
          "header": "X-Frame-Options",
          "value": "DENY",
          "override": true
        },
        {
          "header": "X-Content-Type-Options",
          "value": "nosniff",
          "override": true
        },
        {
          "header": "X-XSS-Protection",
          "value": "1; mode=block",
          "override": true
        },
        {
          "header": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload",
          "override": true
        },
        {
          "header": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin",
          "override": true
        },
        {
          "header": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()",
          "override": true
        }
      ],
      "cors_config": {
        "access_control_allow_credentials": false,
        "access_control_allow_headers": {
          "quantity": 3,
          "items": [
            "Content-Type",
            "Authorization",
            "X-Requested-With"
          ]
        },
        "access_control_allow_methods": {
          "quantity": 7,
          "items": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "HEAD",
            "OPTIONS",
            "PATCH"
          ]
        },
        "access_control_allow_origin": {
          "quantity": 1,
          "items": [
            "*"
          ]
        },
        "access_control_expose_headers": {
          "quantity": 0
        },
        "access_control_max_age": 86400,
        "origin_override": true
      }
    }
  },
  "lambda_functions": [
    {
      "name": "asset-optimization",
      "event_type": "origin-response",
      "lambda_function_arn": "arn:aws:lambda:us-east-1:123456789012:function:aiglossarypro-asset-optimization:1",
      "description": "Optimize asset delivery and add custom headers"
    },
    {
      "name": "security-headers",
      "event_type": "origin-response",
      "lambda_function_arn": "arn:aws:lambda:us-east-1:123456789012:function:aiglossarypro-security-headers:1",
      "description": "Add security headers to all responses"
    }
  ],
  "monitoring": {
    "cloudwatch_metrics": {
      "enabled": true,
      "detailed_metrics": true
    },
    "realtime_metrics": {
      "enabled": true,
      "realtime_metrics_subscription_config": {
        "realtime_metrics_subscription_status": "Enabled",
        "kinesis_stream_config": {
          "role_arn": "arn:aws:iam::123456789012:role/CloudFrontRealtimeLogsRole",
          "stream_arn": "arn:aws:kinesis:us-east-1:123456789012:stream/aiglossarypro-realtime-logs"
        }
      }
    },
    "alarms": [
      {
        "name": "High4xxErrorRate",
        "metric_name": "4xxErrorRate",
        "threshold": 5,
        "comparison_operator": "GreaterThanThreshold",
        "evaluation_periods": 2,
        "alarm_actions": [
          "arn:aws:sns:us-east-1:123456789012:aiglossarypro-alerts"
        ]
      },
      {
        "name": "High5xxErrorRate",
        "metric_name": "5xxErrorRate",
        "threshold": 2,
        "comparison_operator": "GreaterThanThreshold",
        "evaluation_periods": 2,
        "alarm_actions": [
          "arn:aws:sns:us-east-1:123456789012:aiglossarypro-alerts"
        ]
      },
      {
        "name": "LowCacheHitRate",
        "metric_name": "CacheHitRate",
        "threshold": 80,
        "comparison_operator": "LessThanThreshold",
        "evaluation_periods": 3,
        "alarm_actions": [
          "arn:aws:sns:us-east-1:123456789012:aiglossarypro-alerts"
        ]
      }
    ]
  }
}